var comment;
function printdiv(){
        var headstr = "<html><head><title>Booking Details</title><link rel='stylesheet' href='leveltest.css'></head><body>";
        var footstr = "</body></html>";
        var element = document.getElementById("print_div");
        var oldstr = document.body.innerHTML;
        comment = $("#comment_data").val();
        $("#printhelper").html(comment);
        $("#result").addClass("print");

        $("#result").find(".btn_wrap").remove();
        var newstr = element.innerHTML;
        document.body.innerHTML = headstr+newstr+footstr;
        window.print();
        document.body.innerHTML = oldstr;
        $("#comment_data").val(comment);
}
window.addEventListener("afterprint", printAfter);
function printAfter() {
    $("#result").removeClass("print");
}
</script>

